{
  "manifest_version": 3,
  "name": "Get Quizlet Flashcards",
  "description": "Extension for getting flashcards from Quizlet",
  "version": "1.0",
  "content_scripts": [
    {
      "js": ["index.js"],
      "css": ["index.css"],
      "matches": ["https://quizlet.com/*"]
    }
  ],
  "action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
